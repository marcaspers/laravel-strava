{
    "name": "endlessmiles/strava",
    "description": "A laravel package to access data from the Strava API.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.0.1|^7.2",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "EndlessMiles\\Strava\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EndlessMiles\\Strava\\StravaServiceProvider"
            ],
            "aliases": {
                "Strava": "EndlessMiles\\Strava\\StravaFacade"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Caspers",
            "email": "mail@marcocaspers.com"
        }
    ],
    "minimum-stability": "dev"
}
